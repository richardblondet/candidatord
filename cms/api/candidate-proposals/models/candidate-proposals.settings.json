{
  "connection": "default",
  "collectionName": "candidate_proposals",
  "info": {
    "name": "Candidate Proposals"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "proposal": {
      "type": "text",
      "required": true
    },
    "candidate": {
      "model": "candidate",
      "via": "candidate_proposals"
    },
    "categories": {
      "collection": "category",
      "via": "candidate_proposals"
    }
  }
}
